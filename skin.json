{
	"name": "ArchiWiki",
	"version": "1.0.2",
	"author": "Mark Nightingale",
	"url": "https://github.com/Archi-Strasbourg/archi-mediawiki-skin",
	"descriptionmsg": "Theme for 2016 rebuild of Archi-Strasbourg",
	"namemsg": "skinname-archiwiki",
	"license-name": "GPL-3.0",
	"type": "skin",
	"ValidSkinNames": {
		"archiwiki": "ArchiWiki"
	},
	"MessagesDirs": {
		"ArchiWiki": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.archiwiki": {
			"class": "ResourceLoaderSkinModule",
			"position": "top",
			"styles": {
				"dist/css/style.css": {
					"media": ""
				},
				"dist/css/print.css": {
					"media": "print"
				}
			}
		},
		"skins.archiwiki.js": {
			"targets": [
				"mobile",
				"desktop"
			],
			"position": "bottom",
			"scripts": [
				"dist/js/foundation.js",
				"dist/js/scripts.js"
			],
			"dependencies": [
				"ext.maps.leaflet.loader"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "archi-wiki"
	},
	"AutoloadClasses": {
		"SkinArchiWiki": "ArchiWiki.skin.php",
		"ArchiWikiTemplate": "ArchiWikiTemplate.php"
	},
	"manifest_version": 1
}
